{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MainDatabase": "Server=localhost;Database=PayPalPaymentService;TrustServerCertificate=true;Integrated Security=SSPI"
  },
  "PaymentMethod": {
    "PspServiceName": "PSP",
    "PspRegisterApiEndpoint": "/api/PaymentMethod",
